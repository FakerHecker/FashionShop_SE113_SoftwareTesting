{
  "name": "fashionshop",
  "type": "module",
  "version": "1.0.0",
  "description": "FashionShop. nodemon giup auto-restart app khi co su thay doi ma nguon. $ npm start de goi node backend. $ npm run dev de goi nodemon backend",
  "main": "app.js",
  "scripts": {
    "start": "node backend/app.js",
    "dev": "SET NODE_ENV=DEVELOPMENT& nodemon backend/app.js",
    "prod": "SET NODE_ENV=PRODUCTION& nodemon backend/app.js",
    "seeder": "node backend/seeder/seeder.js",
    "prod-build": "SET NODE_ENV=PRODUCTION& node build/app.js",
    "prod-docker": "node build/app.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "nodemon --exec 'npm test'"
  },
  "author": "HungNamAnhQuoc",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "buffer-crc32": "^1.0.0",
    "cloudinary": "^2.1.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "redis": "^4.6.15",
    "stripe": "^15.2.0",
    "util": "^0.12.5",
    "util.promisify": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "babel-jest": "^29.7.0",
    "babel-preset-vite": "^1.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.0.0"
  }
}
